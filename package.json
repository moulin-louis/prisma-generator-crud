{
	"name": "zod-prisma",
	"version": "0.5.4",
	"description": "A Prisma generator that creates Zod schemas for all of your models",
	"license": "MIT",
	"author": "Carter Grimmeisen",
	"homepage": "https://github.com/moulin-louis/prisma-generator-crud.git#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/moulin-louis/prisma-generator-crud.git"
	},
	"bugs": {
		"url": "https://github.com/moulin-louis/prisma-generator-crud/issues"
	},
	"main": "dist/index.js",
	"module": "dist/zod-prisma.esm.js",
	"typings": "dist/index.d.ts",
	"bin": {
		"zod-prisma": "bin/cli.js"
	},
	"keywords": [
		"zod",
		"prisma",
		"generator"
	],
	"files": [
		"bin",
		"dist"
	],
	"scripts": {
		"build": "dts build --target node --format cjs --rollupTypes",
		"lint": "tsc --noEmit && dts lint src --ignore-pattern src/test/functional",
		"prepublish": "dts build --target node --format cjs --rollupTypes",
		"start": "dts watch",
		"test": "dts test --maxWorkers=4 --verbose"
	},
	"prettier": {
		"printWidth": 100,
		"semi": true,
		"singleQuote": true,
		"tabWidth": 2,
		"trailingComma": "es5",
		"useTabs": false
	},
	"eslintConfig": {
		"rules": {
			"react-hooks/rules-of-hooks": "off"
		}
	},
	"dependencies": {
		"@prisma/generator-helper": "~5.22.0",
		"parenthesis": "^3.1.8",
		"ts-morph": "^24.0.0"
	},
	"devDependencies": {
		"@prisma/client": "~5.22.0",
		"@prisma/sdk": "~4.0.0",
		"@tsconfig/recommended": "^1.0.8",
		"@types/fs-extra": "^11.0.4",
		"dts-cli": "^2.0.5",
		"execa": "^9.5.1",
		"fast-glob": "^3.3.2",
		"fs-extra": "^11.2.0",
		"husky": "^9.1.6",
		"jest-mock-extended": "^3.0.7",
		"prisma": "^5.22.0",
		"tslib": "^2.8.1",
		"typescript": "^5.6.3",
		"zod": "^3.23.8"
	},
	"peerDependencies": {
		"decimal.js": "^10.0.0",
		"prisma": "^3.0.0",
		"zod": "^3.0.0"
	},
	"peerDependenciesMeta": {
		"decimal.js": {
			"optional": true
		}
	},
	"engines": {
		"node": ">=14"
	},
	"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
